<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RatioImageView">
        <attr name="width_ratio" format="integer" />
        <attr name="height_ratio" format="integer" />
        <attr name="base" format="enum">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="FocusImageView">
        <attr name="focus_focusing_id" format="integer" />
        <attr name="focus_success_id" format="integer" />
        <attr name="focus_fail_id" format="integer" />
    </declare-styleable>
    <declare-styleable name="startTakePhotoView">
        <attr name="src" format="reference" />
    </declare-styleable>

    <declare-styleable name="MovingImageView">
        <attr name="miv_speed" format="integer"/>
        <attr name="miv_repetitions" format="integer"/>
        <attr name="miv_max_relative_size" format="float"/>
        <attr name="miv_min_relative_offset" format="float"/>
        <attr name="miv_start_delay" format="integer"/>
        <attr name="miv_load_on_create" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="MovingImageViewTheme">
        <attr name="movingImageViewStyle" format="reference"/>
    </declare-styleable>

    <declare-styleable name="VoiceEditTexStytle">
        <attr name="iconVisible" format="boolean"></attr>
    </declare-styleable>




</resources>